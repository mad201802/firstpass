{
  "name": "firstpass",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron ./main.js",
    "dev": "concurrently \"webpack --config webpack.config.js --watch\" \"electronmon ./main.js\""
  },
  "electronmon": {
    "patterns": [
      "app/build/**/*",
      "!**/*.html",
      "!app/src/**/*"
    ]
  },
  "build": {
    "appId": "firstpass",
    "win": {
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "uninstallDisplayName": "Firstpass",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "extraFiles": [
      ""
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@svgr/webpack": "^6.5.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "css-loader": "^6.7.1",
    "electronmon": "^2.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron": "^21.1.1"
  }
}
